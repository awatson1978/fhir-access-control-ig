  fhir:item ( [
     fhir:linkId [ fhir:v "1" ] ;
     fhir:text [ fhir:v "GENERAL INFORMATION AND INSTRUCTIONS: Use this questionnaire if you want to select a person, called an agent, to make future health care decisions for you so that if you become too ill or cannot make those decisions for yourself the person you choose and trust can make medical decisions for you. Be sure you review and understand the importance of the document that is created at the end of this process. It is a good idea to talk to your doctor and loved ones if you have questions about the type of health care you do or do not want." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "1.1" ] ;
     fhir:text [ fhir:v "Selection of my Mental Healthcare Power of Attorney and Alternate: I choose the following person to act as my agent to make mental health care decisions for me." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "1.1.1" ] ;
     fhir:text [ fhir:v "Name" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.1.2" ] ;
     fhir:text [ fhir:v "Address" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.1.3" ] ;
     fhir:text [ fhir:v "Home Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.1.4" ] ;
     fhir:text [ fhir:v "Work Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.1.5" ] ;
     fhir:text [ fhir:v "Cell Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.2" ] ;
     fhir:text [ fhir:v "Selection of my Mental Healthcare Power of Attorney and Alternate: I choose the following person to act as an alternate to make mental health care decisions for me if my first agent is unavailable, unwilling, or unable to make decisions for me." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "1.2.1" ] ;
     fhir:text [ fhir:v "Name" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.2.2" ] ;
     fhir:text [ fhir:v "Address" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.2.3" ] ;
     fhir:text [ fhir:v "Home Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.2.4" ] ;
     fhir:text [ fhir:v "Work Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "1.2.5" ] ;
     fhir:text [ fhir:v "Cell Phone" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "2" ] ;
     fhir:text [ fhir:v "Mental health treatments that I AUTHORIZE if I am unable to make decisions for myself:\n\nHere are the mental health treatments I authorize my agent to make for me if I become incapable of making my own mental health care decisions due to mental or physical illness, injury, disability, or incapacity. This appointment is effective unless and until it is revoked by me or by an order of a court. My agent is authorized to do the following which I have initialed or marked:" ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "2.1" ] ;
     fhir:text [ fhir:v "To receive medical records and information regarding my mental health treatment and to receive, review, and consent to disclosure of any of my medical records related to that treatment.)" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "2.2" ] ;
     fhir:text [ fhir:v "To consent to the administration of any medications recommended by my treating physician." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "2.3" ] ;
     fhir:text [ fhir:v "To admit me to an inpatient or partial psychiatric hospitalization program." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "2.4" ] ;
     fhir:text [ fhir:v "Other:" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "2.4.1" ] ;
     fhir:text [ fhir:v "Other authorizations:" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "3" ] ;
     fhir:text [ fhir:v "Mental health care treatments that I expressly DO NOT AUTHORIZE if I am unable to make decisions for myself: (Explain or write in \"None\")" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "4" ] ;
     fhir:text [ fhir:v "I intend for my agent to be treated as I would be with respect to my rights regarding the use and disclosure of my individually identifiable health information or other medical records. This release authority applies to any information governed by the Health Insurance Portability and Accountability Act of 1996 (aka HIPAA), 42 USC 1320d and 45 CFR 160-164." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "5" ] ;
     fhir:text [ fhir:v "MY SIGNATURE VERIFICATION FOR THE MENTAL HEALTH POWER OF ATTORNEY" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "6" ] ;
     fhir:text [ fhir:v "If you are unable to physically sign this document, your witness/notary may sign and initial for you. If applicable have your witness/notary sign below.\n\nWitness/Notary Verification: The principal of this document directly indicated to me that this Mental Health Power of Attorney expresses their wishes and that they intend to adopt it at this time." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "6.1" ] ;
     fhir:text [ fhir:v "Witness or Notary name" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "6.2" ] ;
     fhir:text [ fhir:v "Witness or Notary signature acquired" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "7" ] ;
     fhir:text [ fhir:v "SIGNATURE OF WITNESS\n\nI was present when this form was signed (or marked). The principal appeared to be of sound mind and was not forced to sign this form. I affirm that I meet the requirements to be a witness as indicated on page one of the mental health power of attorney form." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "7.1" ] ;
     fhir:text [ fhir:v "Witness name" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "7.2" ] ;
     fhir:text [ fhir:v "Witness address" ] ;
     fhir:type [ fhir:v "string" ]
  ] [
     fhir:linkId [ fhir:v "7.3" ] ;
     fhir:text [ fhir:v "Witness signaure acquired" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] ) . # 

