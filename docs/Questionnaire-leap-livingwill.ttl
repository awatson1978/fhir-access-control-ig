     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "2.1" ] ;
     fhir:text [ fhir:v "I HAVE NOT attached additional special instructions about End of Life Care I want." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "2.2" ] ;
