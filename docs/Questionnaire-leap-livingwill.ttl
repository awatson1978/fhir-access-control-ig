  fhir:item ( [
     fhir:linkId [ fhir:v "1" ] ;
     fhir:text [ fhir:v "Some general statements about your health care choices are listed below. If you agree with one ofthe statements, you should select that statement. Read all of these statements carefully BEFORE you select your preferred statement. You can also write your own statement concerning life-sustaining treatment and other matters relating to your health care. You may select any combination of items 1, 2, 3 and 4, BUT if you 'select' item 5 the others will not be selected." ] ;
     fhir:type [ fhir:v "group" ]
  ] [
     fhir:linkId [ fhir:v "1.1" ] ;
     fhir:text [ fhir:v "If I have a terminal condition I do not want my life to be prolonged, and I do not want lifesustaining treatment, beyond comfort care, that would serve only to artificially delay the moment of my death." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "1.2" ] ;
     fhir:text [ fhir:v "If I am in a terminal condition or an irreversible coma or a persistent vegetative state that my doctors reasonably feel to be irreversible or incurable, I do want the medical treatment necessary to provide care that would keep me comfortable, but I DO NOT want the following:" ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "1.3" ] ;
     fhir:text [ fhir:v "Patient Does Not Want the following:" ] ;
     fhir:type [ fhir:v "group" ] ;
     fhir:enableWhen ( [
       fhir:question [ fhir:v "1.2" ] ;
       fhir:operator [ fhir:v "=" ] ;
       fhir:answer [ fhir:v "true"^^xsd:boolean ]
     ] ) ;
     fhir:item ( [
       fhir:linkId [ fhir:v "1.3.1" ] ;
       fhir:text [ fhir:v "Cardiopulmonary resuscitation (CPR). For example: the use of drugs, electric shock and artificial breathing." ] ;
       fhir:type [ fhir:v "boolean" ]
     ] [
       fhir:linkId [ fhir:v "1.3.2" ] ;
       fhir:text [ fhir:v "Artificially administered food and fluids." ] ;
       fhir:type [ fhir:v "boolean" ]
     ] [
       fhir:linkId [ fhir:v "1.3.3" ] ;
       fhir:text [ fhir:v "To be taken to a hospital if at all avoidable." ] ;
       fhir:type [ fhir:v "boolean" ]
     ] )
  ] [
     fhir:linkId [ fhir:v "1.4" ] ;
     fhir:text [ fhir:v "Regardless of any other directions I have given in this Living Will, if I am known to be pregnant, I do not want life-sustaining treatment withheld or withdrawn if it is possible that the embryo/fetus will develop to the point of live birth with the continued application of lifesustaining treatment." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "1.5" ] ;
     fhir:text [ fhir:v "Regardless of any other directions I have given in this Living Will, I do want the use of all medical care necessary to treat my condition until my doctors reasonably conclude that my condition is terminal or is irreversible and incurable or I am in a persistent vegetative state." ] ;
     fhir:type [ fhir:v "boolean" ]
  ] [
     fhir:linkId [ fhir:v "1.6" ] ;
